<div class="col-lg-12">
    {#  START FORM  #}
    {% if  errors is defined and errors is not empty %}
        <div class="alert alert-danger" role="alert">
            {#        {{ errors }}#}
            {{ "Error. Please fill all fields"|trans }}
        </div>
    {% endif %}
    {% if  mess is defined and mess is not empty %}
        <div class="alert alert-success" role="alert">
            {{ mess }}
        </div>
    {% endif %}
